}
<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="mb-3">
        <label>Fecha: @Model.fechaVisita</label>
    </div>
    <div class="mb-3">
        <label>Id(miembro): @Model.miembroId</label>
    </div>
    
    <div class="mb-3">
        <label>Descripci√≥n: @Model.descripcionVisita</label>
    </div>    

    <div class="row col-6">
