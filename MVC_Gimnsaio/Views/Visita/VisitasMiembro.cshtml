@model List<MVC_Gimnsaio.Models.Visita>

@{
    ViewData["Title"] = "Lista de Visitas por miembro";
}

<h1>@ViewData["Title"]</h1>

<table class="table table-hover">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Fecha:</th>
        <th scope="col">Id(miembro):</th>
        <th scope="col">Descripci√≥n:</th>        
    </tr>
    </thead>
    
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <th scope="row">@item.idVisita</th>
            <td>@item.fechaVisita</td>
            <td>@item.miembroId</td>
            <td>@item.descripcionVisita</td>
        </tr>
    }
    </tbody>
</table>

<p>Esta es la vista de la lista de visitas por miembro</p>7