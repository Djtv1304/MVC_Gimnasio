@model Miembro
@{
    ViewData["Title"] = "Detalle Miembro";
}
<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="mb-3">
        <label>Nombre: @Model.nombreMiembro @Model.apellidoMiembro</label>
    </div>
<div class="mb-3">
        <label>E-mail: @Model.emailMiembro</label>
    </div>
    
    <div class="mb-3">
        <label>Fecha inscripci√≥n: @Model.fechaInscripcion</label>
    </div>    

    <div class="row col-6">
        <div class="col-6 col-md-6">
            <a asp-action="Index" class="btn btn-outline-secondary">Regresar</a>
        </div>
    </div>
</form>

<p>Esta es la vista para ver detalles del miembro</p>