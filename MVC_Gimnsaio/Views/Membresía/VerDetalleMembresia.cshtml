@model Membresia
@{
    ViewData["Title"] = "Detalle de Membresía";
}
<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="mb-3">
        <label>Tipo: @Model.duracionMembresia</label>
    </div>
<div class="mb-3">
        <label>Duración: @Model.duracionMembresia</label>
    </div>
    
    <div class="mb-3">
        <label>Precio: @Model.precioMembresia</label>
    </div>    

    <div class="row col-6">
        <div class="col-6 col-md-6">
            <a asp-action="Index" class="btn btn-outline-secondary">Regresar</a>
        </div>
    </div>
</form>

<p>Esta es la vista para ver detalles de la membresía</p>
